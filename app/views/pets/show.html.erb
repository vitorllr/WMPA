<%= link_to '<i class="fa-solid fa-arrow-left mt-5 ms-5 black"> <span class="ms-2"> Back </span></i>'.html_safe, pets_path %>
<div class="lg-container">
  <div class="text-center title mb-4"><%= @pet.name %></div>
  <div id="carouselExample" class="carousel slide mb-3">
    <div class="carousel-inner">
      <% @pet.photos.each do |photo| %>
        <% if photo.key == @pet.photos.first.key %>
          <div class="carousel-item active">
            <%= cl_image_tag photo.key, crop: :fill, class: "d-block w-100 lg-img" %>
          </div>
        <% else %>
          <div class="carousel-item ">
            <%= cl_image_tag photo.key, crop: :fill, class: "d-block w-100 lg-img" %>
          </div>
        <% end %>
      <% end %>
    </div>
    <button class="carousel-control-prev" type="button" data-bs-target="#carouselExample" data-bs-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="visually-hidden">Previous</span>
    </button>
    <button class="carousel-control-next" type="button" data-bs-target="#carouselExample" data-bs-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="visually-hidden">Next</span>
    </button>
  </div>
  <div class="lg-container">
    <div class="dog-infos mt-3">
      <span class="text-center ms-5 me-5">Breed: <%= @pet.breed %></span>
      <span class="text-center ms-5 me-5">Gender: <%= @pet.gender %></span>
      <span class="text-center ms-5 me-5">Age: <%= @pet.age %></span>
      <span class="text-center">Size: <%= @pet.size %></span>
    </div>
  </div>
</div>
